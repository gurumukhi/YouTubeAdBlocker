{
  "manifest_version": 2,
  "name": "AdBlocker for YouTube",
  "description": "Removes all annoying Ads and Banners from YouTube",
  "version": "0.1.2",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options/index.html"
  },
  "permissions": [
    "storage"
  ]
}