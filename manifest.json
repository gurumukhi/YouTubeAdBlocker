{
  "manifest_version": 2,
  "name": "AdBlocker for YouTube",
  "description": "Removes all annoying Ads and Banners from YouTube",
  "version": "0.0.1",
  "icons": {
    "64": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "content_script.js"
      ]
    }
  ]
}